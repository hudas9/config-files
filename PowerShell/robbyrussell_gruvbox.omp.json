{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#98971A",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        },
        {
          "foreground": "#458588",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#FB4934",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<#5FAAE8>git:(</>{{ .HEAD }}<#5FAAE8>) </>",
          "type": "git"
        },
        {
          "foreground": "#D65D0E",
          "style": "plain",
          "template": "\u0078 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": false,
  "version": 2
}
