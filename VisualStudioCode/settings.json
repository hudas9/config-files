{
  "breadcrumbs.enabled": false,
  "codesnap.containerPadding": "0",
  "codesnap.showWindowControls": false,
  "codesnap.showWindowTitle": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Hack Nerd Font, JetBrainsMono Nerd Font, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.inlineSuggest.enabled": true,
  "prettier.singleQuote": false,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.statusBar.visible": false,
  "workbench.colorTheme": "Better Solarized Dark",
  "material-icon-theme.folders.color": "#9eacad",
  "workbench.iconTheme": "material-icon-theme",
  "window.commandCenter": false,
  "workbench.editor.enablePreview": true,
  "workbench.startupEditor": "none",
  "editor.formatOnSave": true,
  "update.showReleaseNotes": false,
  "workbench.colorCustomizations": {
    "editorLineNumber.activeForeground": "#fdf6e3"
  },
  "workbench.sideBar.location": "right",
  "window.menuBarVisibility": "hidden",
  "workbench.layoutControl.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.colorDecorators": false,
  "editor.wordWrap": "on"
}
